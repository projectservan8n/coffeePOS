    {
      "parameters": {
        "functionCode": "// Generate weekly business report\nconst data = $input.first().json;\nconst analytics = data.analytics;\nconst settings = $node[\"Get Settings\"].json;\n\n// Format currency\nconst formatCurrency = (amount) => `‚Ç±${amount.toLocaleString('en-PH', {minimumFractionDigits: 2, maximumFractionDigits: 2})}`;\n\n// Calculate key metrics\nconst avgDailyRevenue = analytics.totalRevenue / 7;\nconst avgDailyOrders = analytics.totalOrders / 7;\n\n// Generate insights\nconst insights = [];\nif (analytics.totalOrders === 0) {\n  insights.push('‚Ä¢ No orders recorded this week - check system connectivity');\n} else {\n  if (analytics.avgOrderValue > 150) {\n    insights.push('‚Ä¢ Excellent average order value - customers are buying premium items');\n  } else if (analytics.avgOrderValue < 100) {\n    insights.push('‚Ä¢ Consider upselling strategies to increase average order value');\n  }\n  \n  if (analytics.profitMargin > 70) {\n    insights.push('‚Ä¢ Outstanding profit margins - great cost management');\n  } else if (analytics.profitMargin < 50) {\n    insights.push('‚Ä¢ Review ingredient costs and pricing strategy');\n  }\n  \n  if (analytics.totalOrders > 100) {\n    insights.push('‚Ä¢ High order volume - consider optimizing operations for efficiency');\n  }\n}\n\n// Top products section\nconst topProductsText = analytics.topProducts && analytics.topProducts.length > 0 \n  ? analytics.topProducts.map(p => `‚Ä¢ ${p.name}: ${p.quantity} sold`).join('\\n')\n  : 'No product data available';\n\nconst emailSubject = `üìà Weekly Business Report - ${settings.shopName || 'Coffee Shop'}`;\n\nconst emailBody = `\nGood morning!\n\nHere's your weekly performance summary for ${settings.shopName || 'your coffee shop'}.\n\nüí∞ FINANCIAL SUMMARY (Last 7 Days):\n‚Ä¢ Total Revenue: ${formatCurrency(analytics.totalRevenue)}\n‚Ä¢ Total Orders: ${analytics.totalOrders.toLocaleString()}\n‚Ä¢ Average Order Value: ${formatCurrency(analytics.avgOrderValue)}\n‚Ä¢ Total Profit: ${formatCurrency(analytics.totalProfit)}\n‚Ä¢ Profit Margin: ${analytics.profitMargin.toFixed(1)}%\n\nüìä DAILY AVERAGES:\n‚Ä¢ Revenue per day: ${formatCurrency(avgDailyRevenue)}\n‚Ä¢ Orders per day: ${avgDailyOrders.toFixed(1)}\n\nüèÜ TOP SELLING PRODUCTS:\n${topProductsText}\n\nüí° BUSINESS INSIGHTS:\n${insights.join('\\n') || '‚Ä¢ Performance metrics are within normal ranges'}\n\nüìÖ WEEK AHEAD RECOMMENDATIONS:\n‚Ä¢ Monitor top-selling items for stock availability\n‚Ä¢ Review slow-moving inventory for promotions\n‚Ä¢ Analyze peak hours for staff scheduling\n‚Ä¢ Consider seasonal menu adjustments\n\nReport Period: ${new Date(Date.now() - 7 * 24 * 60 * 60 * 1000).toLocaleDateString()} - ${new Date().toLocaleDateString()}\nGenerated: ${new Date().toLocaleString('en-PH', {timeZone: 'Asia/Manila'})}\n\nKeep up the great work!\nCoffee POS Analytics Team\n`;\n\nreturn {\n  json: {\n    to: settings.ownerEmail || 'owner@coffeeshop.com',\n    subject: emailSubject,\n    body: emailBody,\n    revenue: analytics.totalRevenue,\n    orders: analytics.totalOrders,\n    profitMargin: analytics.profitMargin\n  }\n};"
      },
